server {
	listen  80;
if ($http_user_agent ~* Baiduspider){
return 403 http://www.hellonet8.com$request_uri;
}
	server_name hb.wmdev.lsh123.com;
	root	/home/work/code/Avatar/yaf/fe-test-platform-hb/webroot;	
	location /static/ {
		root /home/work/code/Avatar/yaf/fe-test-platform-hb/webroot/;
	}
	include fe_server_base;
}

