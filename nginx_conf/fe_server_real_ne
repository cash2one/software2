server {
    listen  80;
if ($http_user_agent ~* Baiduspider){
return 403 http://www.hellonet8.com$request_uri;
}
    server_name ne.wmdev2.lsh123.com
        charset utf-8;
    root    /home/work/code/Avatar/yaf/fe-test-platform-ne/webroot; 
    location /static/ {
        root /home/work/code/Avatar/yaf/fe-test-platform-ne/webroot/;
    }
    include fe_server_base;
}


