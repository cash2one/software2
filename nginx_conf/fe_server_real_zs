server {
    listen  80;
if ($http_user_agent ~* Baiduspider){
return 403 http://www.hellonet8.com$request_uri;
}
    server_name zs.market-mis.wmdev2.lsh123.com;
    root    /home/work/test-env/market-mis/fe-test-zs/webroot;
    location /static/ {
        root /home/work/test-env/market-mis/fe-test-zs/webroot;
    }
    include fe_server_base;
}

